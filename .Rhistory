for (i in 1:50){
target <- rank[[i]] %>% gsub("^\\s+|\\s+$|\n| ","",.)
if ((i >= 1) & (i <= 9)) {
stat <- substr(target, (nchar(target)-i), nchar(target))
if (!(stat %in% c('유지','ew'))) {
current_rank <- substr(target, 1, 1)
diff <- substr(target %>% gsub(stat,'',.), 2, 3)
} else if (stat == '유지') {
current_rank = substr(target, 1, 1)
diff = 0
}
}
}
